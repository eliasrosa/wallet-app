{
	"name": "finance-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "npx @biomejs/biome lint",
		"lint:fix": "npx @biomejs/biome check --write",
		"db:migrate": "npx prisma migrate dev",
		"db:reset": "npx prisma migrate reset --force && npm run db:migrate && npm run db:seed",
		"db:reset2": "rm -rf prisma/migrations && npx prisma migrate reset --force && npm run db:migrate -- --name init && npm run db:seed",
		"b3:import-dividends": "npx tsx ./src/commands/b3/ImportDividendsCommand.ts",
		"b3:import-movements": "npx tsx ./src/commands/b3/ImportMovementsCommand.ts",

		"db:seed": "npx tsx ./src/commands/SeedCommand.ts",

		"import:ticker": "npx tsx ./src/commands/ImportTickerDataCommand.ts",
		"wallet:create": "npx tsx ./src/commands/WalletCreateCommand.ts"
	},
	"dependencies": {
		"@prisma/client": "^6.3.1",
		"@radix-ui/react-dialog": "^1.1.6",
		"@radix-ui/react-dropdown-menu": "^2.1.6",
		"@radix-ui/react-label": "^2.1.2",
		"@radix-ui/react-separator": "^1.1.2",
		"@radix-ui/react-slot": "^1.1.2",
		"@radix-ui/react-tooltip": "^1.1.8",
		"cheerio": "^1.0.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"inversify": "^6.2.2",
		"inversify-inject-decorators": "^3.1.0",
		"lodash": "^4.17.21",
		"lucide-react": "^0.475.0",
		"next": "15.1.7",
		"next-themes": "^0.4.4",
		"object-hash": "^3.0.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"read-excel-file": "^5.8.6",
		"recharts": "^2.15.1",
		"reflect-metadata": "^0.2.2",
		"tailwind-merge": "^3.0.1",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@eslint/eslintrc": "^3",
		"@types/lodash": "^4.17.15",
		"@types/node": "^20",
		"@types/object-hash": "^3.0.6",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "15.1.7",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.3",
		"postcss": "^8",
		"prettier": "3.5.1",
		"prisma": "^6.3.1",
		"tailwindcss": "^3.4.1",
		"typescript": "^5"
	}
}
